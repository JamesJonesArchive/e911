{
    "name": "usf/e911",
    "description": "University of South Florida 911 and E911 Disclosure, Notice, and Acknowledgement",
    "authors": [
        {
            "name": "James Jones",
            "email": "james@mail.usf.edu",
            "role": "Developer"
        }
    ],
    "config": {
        "vendor-dir-legacy": "app/api/vendor",
        "component-dir-legacy": "app/api/components"
    },
    "autoload": {
        "psr-4" : {
            "USF\\e911\\" : "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        { 
            "type":"vcs",
            "url":"https://github.com/USF-IT/usf-idm-common.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "slim/slim": "2.5.0",
        "usf-it/usf-idm-common": "0.3.0",
        "NanneHuiges/JSend": "2.1.0",
        "guzzlehttp/guzzle": "5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.6",
        "squizlabs/php_codesniffer": "2.1.0",
        "guzzlehttp/guzzle": "5.3.0"
    }
}
